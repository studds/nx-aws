{
    "name": "nx-aws",
    "version": "0.12.2",
    "description": "AWS plugins for nx",
    "keywords": [
        "AWS",
        "SAM",
        "Serverless",
        "Angular CLI",
        "Workspace",
        "Nx",
        "Monorepo",
        "s3"
    ],
    "scripts": {
        "nx": "nx",
        "ts-node": "TS_NODE_PROJECT=tsconfig.node.json cross-dotenv ts-node -r tsconfig-paths/register",
        "start": "nx serve",
        "build": "nx run-many --target=build --all",
        "pack": "nx run-many --target=pack --all",
        "pack:copy": "yarn ts-node ./scripts/cpx.ts",
        "semver:dryrun": "yarn semver --dry-run",
        "semver": "nx run workspace:semver",
        "publish:latest": "NPM_TAG=latest nx run-many --target=publish --all",
        "publish:next": "NPM_TAG=next nx run-many --target=publish --all",
        "test": "nx run-many --target=test --all",
        "lint": "nx workspace-lint && nx run-many --target=lint --all",
        "e2e": "nx e2e",
        "affected:apps": "nx affected:apps",
        "affected:libs": "nx affected:libs",
        "affected:build": "nx affected:build",
        "affected:e2e": "nx affected:e2e",
        "affected:test": "nx affected:test",
        "affected:lint": "nx affected:lint",
        "affected:dep-graph": "nx affected:dep-graph",
        "affected": "nx affected",
        "format": "nx format:write",
        "format:write": "nx format:write",
        "format:check": "nx format:check",
        "update": "nx migrate latest",
        "dep-graph": "nx dep-graph",
        "help": "nx help",
        "workspace-generator": "nx workspace-generator"
    },
    "author": "studds@fastmail.com",
    "license": "MIT",
    "dependencies": {
        "@angular-devkit/architect": "0.1102.1",
        "@angular-devkit/build-webpack": "0.1102.1",
        "@angular-devkit/core": "11.2.1",
        "@angular-devkit/schematics": "11.2.1",
        "@nrwl/node": "^12",
        "@nrwl/nx-plugin": "^12",
        "@nrwl/workspace": "^12",
        "@types/aws-lambda": "^8.10.62",
        "aws-sdk": "^2.850.0",
        "cloudform-types": "^6.1.0",
        "cloudformation-js-yaml-schema": "^0.4.2",
        "cross-spawn": "^7.0.3",
        "js-yaml": "^4.0.0",
        "mkdirp": "^1.0.4",
        "rxjs": "^6.6.0",
        "source-map-support": "^0.5.19",
        "strip-json-comments": "^3.1.0",
        "ts-essentials": "^7.0.0",
        "webpack-merge": "^5.1.1"
    },
    "devDependencies": {
        "@babel/core": "^7.13.1",
        "@jscutlery/semver": "^1.3.1",
        "@nrwl/cli": "^12",
        "@nrwl/eslint-plugin-nx": "^12",
        "@nrwl/jest": "^12",
        "@nrwl/tao": "^12",
        "@types/cpx": "^1.5.1",
        "@types/cross-spawn": "^6.0.2",
        "@types/jest": "26.0.20",
        "@types/js-yaml": "^4.0.0",
        "@types/mkdirp": "^1.0.0",
        "@types/node": "12.12.38",
        "@types/terser-webpack-plugin": "^5.0.2",
        "@types/webpack-dev-server": "^3.10.1",
        "@types/yargs": "^16.0.0",
        "@typescript-eslint/eslint-plugin": "4.15.2",
        "@typescript-eslint/parser": "4.15.2",
        "cpx": "^1.5.0",
        "dotenv": "^8.2.0",
        "eslint": "7.20.0",
        "eslint-config-prettier": "^8.0.0",
        "eslint-plugin-prettier": "^3.1.4",
        "jest": "26.6.3",
        "monorepo-package-tool": "^2.0.1",
        "npm-run-all": "^4.1.5",
        "prettier": "2.2.1",
        "prettier-eslint": "^12.0.0",
        "ts-jest": "26.5.2",
        "ts-node": "9.1.1",
        "tsconfig-paths": "^3.9.0",
        "typescript": "4.0.7",
        "webpack": "4",
        "yargs": "^16.2.0"
    },
    "repository": "https://github.com/studds/nx-aws",
    "publishConfig": {
        "registry": "https://registry.npmjs.org/"
    },
    "engines": {
        "npm": "please-use-yarn",
        "yarn": ">= 1.22.10",
        "node": ">= 14.6.1"
    }
}
